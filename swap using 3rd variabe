import java.util.*;

class xysx
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter the three numbers");
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=b;
        b=a;
        a=c;
        
        System.out.println("after swapping a = "+a+" b = "+b);
            
        
    }
}
