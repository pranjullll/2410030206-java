import java.util.*;

class xysx
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter the three numbers");
        int a=sc.nextInt();
        int b=sc.nextInt();
        if(a>0&&b>0)
        {
            System.out.println("the result is +ve");
            
        }
        else{
            System.out.println("the result is -ve");
        }
            
        
    }
}
